{
  "name": "project-eagle",
  "version": "0.1.0",
  "bin": {
    "project-eagle": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "lint": "eslint . --ext .ts",
    "cdk-build": "npm run build && npm run lint && cdk synth",
    "build:cdk": "tsc && eslint . --ext .ts && cdk synth",
    "deploy:cdk": "npm run build:cdk && cdk deploy",
    "build:frontend": "cd frontend && npm run build",
    "deploy:frontend": "cd frontend && npm run build && npm run deploy"
  },
  "engines": {
    "node": "20.19.0",
    "npm": ">=10.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.7.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "aws-cdk": "2.88.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "@aws-amplify/adapter-nextjs": "^1.6.2",
    "aws-amplify": "^6.14.4",
    "aws-cdk-lib": "2.88.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
